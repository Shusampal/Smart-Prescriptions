<div class="row m-3">
  <h4 class="card-title">
    <%= data.medications[1].treatment %>
  </h4>
  <div class="col-sm-3 m-3">
    <div class="card" style="height: 300px;">
      <div class="card-body">
        <img src="./images/syringe.png" class="card-img-top" alt="" style="width: 70px;">
        <br><br>
        <h5 class="card-title" style="color: grey;">
          Reason for Medication
        </h5>
        <p class="card-text"><%= data.medications[1].reason %> <br> <br> <br></p>
        
      </div>
    </div>
  </div>

  <div class="col-sm-3 m-3">
    <div class="card" style="height: 300px;">
      <div class="card-body">
        <h5 class="card-title" style="color: grey;">
          Directions/Notes
        </h5>
        <p class="card-text">
          <%= data.medications[1].doseperday %>.<br/>
          <%= data.medications[1].direction %>
          
        </p>

      
        <h4>Dose Timing : </h4>
        <%= data.medications[1].time[0]%> : <img src="./images/pm.png" class="card-img-top" alt="" style="width: 40px;">

        <br><br><br>
  
      </div>
    </div>
  </div>


  <div class="col-sm-3 m-3">
    <div class="card" style="height: 300px;">
      <div class="card-body">
        <h5 class="card-title" style="color: grey;">
          possible Side Effects
        </h5>
        <p class="card-text">
          <%= data.medications[1].possibleSideeffects[0]%> <img src="./images/headache.png" class="card-img-top" alt="" style="width: 28px;">
        </p>

        <p class="card-text">
          <%= data.medications[1].possibleSideeffects[1]%> <img src="./images/fatigue.png" class="card-img-top" alt=""
              style="width: 28px;">
        </p>

        <p class="card-text">
          <%= data.medications[1].possibleSideeffects[2]%> <img src="./images/vomit.png" class="card-img-top" alt=""
              style="width: 28px;">
        </p>

        <p>
          <span style="color: grey;">GET MEDICAL HELP IF </span><%= data.medications[1].remedy%>
        </p>
  
       
  
      </div>
    </div>
  </div>
  
</div>